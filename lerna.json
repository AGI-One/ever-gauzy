{
	"npmClient": "yarn",
	"version": "0.1.0",
	"changelog": {
		"repo": "ever-co/ever-gauzy",
		"cacheDir": ".changelog",
		"labels": {}
	},
	"packages": [
		"apps/*",
		"packages/*",
		"packages/modules/*",
		"packages/modules/providers/*",
		"packages/plugins/*"
	],
	"useNx": true,
	"command": {
		"exec": {
			"scope": ["@ngx-starter-kit/*"]
		},
		"clean": {
			"yes": true
		}
	},
	"npmClientArgs": ["--no-package-lock"]
}
